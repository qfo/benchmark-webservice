	<div class="row">
		<div class="col-md-3 ">
			<strong>Upload Date</strong>
		</div>
		<div class="col-md-9 ">
			<DVAR name="uploadDate">
			</div>
	</div>

	<div class="row">
		<div class="col-md-3 ">
			<strong>Coverage</strong>
		</div>
		<div class="col-md-9 ">
			<DVAR name="coverage">
		</div>
